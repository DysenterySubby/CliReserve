<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/constraintLayout1"
    android:minWidth="25px"
    android:minHeight="25px"
    android:layout_marginTop="1dp"
    android:layout_marginBottom="1dp"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@android:color/white">

    <TextView
        android:id="@+id/tvTypeName"
        android:text="Trapezoid Folding Table"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
        android:textStyle="bold"
        android:textSize="14sp"
        android:textAlignment="viewStart"
        android:textColor="@color/colorAccent"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        app:layout_constraintBottom_toTopOf="@+id/tvSeatId"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tvSeatId"
        android:text="R-TFT"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
        android:textStyle="bold"
        android:textSize="14sp"
        android:textAlignment="viewStart"
        android:textColor="@color/colorPrimary"
        app:layout_constraintBottom_toTopOf="@+id/txtBookinID"
        app:layout_constraintEnd_toEndOf="@+id/tvTypeName"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/tvTypeName"
        app:layout_constraintTop_toBottomOf="@+id/tvTypeName" />

    <TextView
        android:id="@+id/txtBookinID"
        android:text="Booking ID:"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="10sp"
        android:fontFamily="sans-serif-medium"
        android:layout_marginTop="8dp"
        app:layout_constraintStart_toStartOf="@+id/tvTypeName"
        app:layout_constraintTop_toBottomOf="@+id/tvSeatId" />

    <TextView
        android:id="@+id/tvBookingId"
        android:text="2022153910-13-07-2024-07-34"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="10sp"
        android:fontFamily="sans-serif-medium"
        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/tvTypeName"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/txtBookinID"
        app:layout_constraintTop_toBottomOf="@+id/txtBookinID" />

    <TextView
        android:id="@+id/txtFrom"
        android:text="From:"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="10sp"
        android:fontFamily="sans-serif-medium"
        android:layout_marginEnd="24dp"
        app:layout_constraintBottom_toTopOf="@+id/tvStartTime"
        app:layout_constraintEnd_toEndOf="@+id/tvStartTime"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/tvTypeName"
        app:layout_constraintTop_toTopOf="@+id/tvTypeName" />

    <TextView
        android:id="@+id/tvStartTime"
        android:text="2:00PM"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="12sp"
        android:fontFamily="sans-serif-medium"
        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
        android:layout_marginEnd="8dp"
        app:layout_constraintBottom_toTopOf="@+id/txtTo"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtFrom" />

    <TextView
        android:id="@+id/txtTo"
        android:text="To:"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="10sp"
        android:fontFamily="sans-serif-medium"
        android:layout_marginEnd="24dp"
        app:layout_constraintBottom_toTopOf="@+id/tvEndTime"
        app:layout_constraintEnd_toEndOf="@+id/tvStartTime"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/txtFrom"
        app:layout_constraintTop_toBottomOf="@+id/tvStartTime" />

    <TextView
        android:id="@+id/tvEndTime"
        android:text="2:00PM"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="12sp"
        android:fontFamily="sans-serif-medium"
        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
        app:layout_constraintBottom_toBottomOf="@+id/tvBookingId"
        app:layout_constraintEnd_toEndOf="@+id/tvStartTime"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtTo" />

</androidx.constraintlayout.widget.ConstraintLayout>